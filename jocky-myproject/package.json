  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.config.client.js",
    "build": "npm run clean && npm run build:client",
    "clean": "rimraf dist",
    "dev": "npm run lint-fix && cross-env NODE_ENV=development webpack-dev-server --config build/webpack.config.client.js",
    "lint": "eslint --ext .js --ext .jsx --ext .vue client/",
    "lint-fix": "eslint --fix --ext .js --ext .jsx --ext .vue client/",
    "precommit": "npm run lint-fix",
    "practice": "eslint --fix --ext .js --ext .jsx --ext .vue practice/ && cross-env NODE_ENV=development webpack-dev-server --config build/webpack.config.practice.js"
  },
